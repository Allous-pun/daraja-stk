{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/validation.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/confirmation.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/validation",
      "dest": "/api/validation.js"
    },
    {
      "src": "/api/confirmation",
      "dest": "/api/confirmation.js"
    },
    {
      "src": "/api/c2b-payment",
      "dest": "/server.js"
    }
  ]
}
